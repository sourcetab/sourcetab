{
  "name": "web-launcher",
  "version": "2.4.2",
  "description": "A material design dashboard that replaces your new tab page and improves productivity.",
  "scripts": {
    "dev": "webpack s --env mode=prod --env platform=web --open",
    "dev:chrome": "webpack --env mode=dev --env platform=chrome --watch",
    "dev:firefox": "webpack --env mode=dev --env platform=firefox --watch",
    "build": "npm run build:web; npm run build:chrome; npm run build:firefox;",
    "build:web": "webpack --env mode=prod --env platform=web",
    "build:chrome": "webpack --env mode=prod --env platform=chrome",
    "build:firefox": "webpack --env mode=prod --env platform=firefox",
    "format": "prettier -w .",
    "lint": "eslint --fix .",
    "analyze": "webpack --env mode=prod --env platform=web --analyze"
  },
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-sortablejs": "^6.0.0",
    "sortablejs": "^1.14.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-extension-manifest-plugin": "^0.8.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-dev-server": "^4.2.1"
  }
}
